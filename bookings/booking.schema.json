{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"type": "object",
	"properties": {
		"type": {
			"type": "string",
			"enum": ["booking"]
		},
		"slot": {
			"type": ["null", "string"]
		},
		"event": {
			"type": "string"
		},
		"date": {
			"type": "string",
			"format": "datetime"
		},
		"name": {
			"type": "string"
		},
		"email": {
			"type": ["null", "string"]
		},
		"id_user": {
			"type": ["null", "integer"]
		},
		"fields": {
			"type": ["null", "object"],
			"items": {
				"type": "string"
			}
		},
		"seats": {
			"type": "integer",
			"minimum": 1
		}
	},
	"required": ["type", "event", "slot", "date", "name", "email", "id_user", "fields"]
}